<commands>
  <command name="User.Query" type="text" connection="local">
    <![CDATA[
SELECT [ID]
      ,[LoginID]
      ,[Phone]
      ,[Name]
      ,[Group]
      ,[Enabled]
      ,[CreateTime]
      ,[LastLoginIP]
      ,[LastLoginTime]
  FROM [dbo].[User] WHERE [Deleted]=0 
      ]]>
